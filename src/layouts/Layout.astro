---
import "../styles/style.css";

interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="EeEE E e EEE eEEEEe EEeE EeEE EE EeeE EeeE EE eE eeE eEEEEe eeE eee" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
    <meta
      name="theme-color"
      content="#00CCCC"
      media="(prefers-color-scheme: light)"
    />
    <meta
      name="theme-color"
      content="#003366"
      media="(prefers-color-scheme: dark)"
    />
	</head>
	<body>
		<slot />
	</body>
</html>